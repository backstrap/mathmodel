{
    "plugins": [],
    "opts": {
        "recurse": true,
        "encoding": "utf8",
        "destination": "docs"
    },
    "source": {
        "include" : ["src", "README.md"],
        "includePattern": ".+\\.(js|md)$"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        }
    }
}
